<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="#789456">

    <ImageView
        android:id="@+id/bubbleIv1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="50dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:src="@mipmap/wallet_bubble1"
        />

    <ImageView
        android:id="@+id/bubbleIv2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv1"
        android:src="@mipmap/wallet_bubble2"
        />
    <ImageView
        android:id="@+id/bubbleIv3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="80dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_toRightOf="@id/bubbleIv1"
        android:src="@mipmap/wallet_bubble3"
        />

    <ImageView
        android:id="@+id/bubbleIv4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv3"
        android:layout_marginRight="10dp"
        android:layout_alignParentRight="true"
        android:src="@mipmap/wallet_bubble4"
        />

    <ImageView
        android:id="@+id/bubbleIv5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv3"
        android:layout_marginRight="10dp"
        android:layout_toRightOf="@id/bubbleIv2"
        android:src="@mipmap/wallet_bubble5"
        />

    <ImageView
        android:id="@+id/bubbleIv6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv2"
        android:src="@mipmap/wallet_bubble6"
        />

    <ImageView
        android:id="@+id/bubbleIv7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv5"
        android:layout_toRightOf="@id/bubbleIv6"
        android:src="@mipmap/wallet_bubble7"
        />

    <ImageView
        android:id="@+id/bubbleIv8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv4"
        android:layout_toRightOf="@id/bubbleIv7"
        android:src="@mipmap/wallet_bubble8"
        />
    <ImageView
        android:id="@+id/bubbleIv9"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv6"
        android:src="@mipmap/wallet_bubble9"
        />
    <ImageView
        android:id="@+id/bubbleIv10"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="80dp"
        android:layout_alignParentRight="true"
        android:background="@mipmap/wallet_bubble_bg"
        android:layout_below="@id/bubbleIv7"
        android:src="@mipmap/wallet_bubble10"
        />


    <com.app.root.wallet.NxnWalletView
        android:id="@+id/walletView"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="110dp"/>

    <LinearLayout
        android:id="@+id/bottomLayout"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="87dp"
        android:layout_alignParentBottom="true"
        android:gravity="center_horizontal"
        android:background="#004C3F">
        <TextView
            android:id="@+id/bootomTypeTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ethereum"
            android:textColor="#ffffffff"
            android:textSize="24sp"
            />
        <View
            android:layout_width="134dp"
            android:layout_height="5dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/round_line_white_c2"/>
    </LinearLayout>

</RelativeLayout>
