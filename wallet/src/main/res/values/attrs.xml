<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ContactItemView">
        <attr name="contactItemImage" format="reference"/>
        <attr name="contactItemName" format="string"/>
    </declare-styleable>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="textSizes" format="dimension|reference"/>
        <attr name="textColorOut" format="color"/>
        <attr name="textColorCenter" format="color"/>
        <attr name="dividerColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="Title_bar">
        <attr name="titleText" format="string"/>
        <attr name="righttitleText" format="string"/>
        <attr name="setleftimg" format="boolean"/>
        <attr name="leftimg" format="reference|integer"/>
        <attr name="rightimg" format="reference|integer"/>
        <attr name="backgroundcolor" format="integer"/>
    </declare-styleable>
    <declare-styleable name="PullScrollView">
        <attr name="header" format="reference" />
        <attr name="headerHeight" format="dimension" />
        <attr name="headerTopHeight" format="dimension"/>
        <attr name="headerVisibleHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" format="dimension"/>
        <attr name="type">
            <enum name="circle" value="0"/>
            <enum name="round" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LazyViewPager">
        <attr name="init_lazy_item_offset" format="float"/>
    </declare-styleable>
</resources>