        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="ActivityInOutAnimation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/push_r2l_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_r2l_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_l2r_in</item>
        <item name="android:activityCloseExitAnimation">@anim/push_l2r_out</item>
    </style>

    <style name="AnimationPreview">
