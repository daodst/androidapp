        <!--<item name="app:tabSelectedTextColor">@color/default_tab_text_selected_color</item>-->
        <!--<item name="app:tabTextColor">@color/main_tb_vt</item>-->
    </style>

    <style name="WelAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
